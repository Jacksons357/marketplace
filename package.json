{
  "name": "marketplace-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "docker:migrate": "docker exec -it marketplace_api npx prisma migrate dev --schema src/infra/database/prisma/schema.prisma",
    "docker:seed": "docker exec -it marketplace_api npx prisma db seed --schema src/infra/database/prisma/schema.prisma",
    "docker:restart": "docker compose down && docker compose up --build -d",
    "prisma:migrate": "npx prisma migrate deploy --schema src/infra/database/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema src/infra/database/prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node src/infra/database/prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "5.1.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "ollama": "^0.6.2",
    "openai": "^6.7.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "pino-pretty": "^13.1.2",
    "prisma": "5.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
